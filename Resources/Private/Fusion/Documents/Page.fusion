
prototype(Neos.Neos:Page) {
    captchaEUWidgetScript = Neos.Fusion:Tag {
        @position = 'after closingHeadTag'

        tagName = 'script'
        attributes {
            nomodule = true
            src = 'https://w19.captcha.at/sdk.js'
            async = true
            defer = true
        }
    }
}
